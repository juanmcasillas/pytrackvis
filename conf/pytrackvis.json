{
    "verbose": true,
    "points": {
        "optimize": true,
        "filter": true
    },
    "database": {
        "file": "db/trackdb.db",
        "script": "db/trackdb.sql"
    },
    "proxy": {
        "enabled": true,
        "url": "http://proxy.senado.es:8080"
    },
    "api_key_file": "conf/tokens.ini",
    "logs": {
        "level": "INFO",
        "format": "[%(asctime)s] %(levelname)s in %(module)s: %(message)s",
        "app": "logs/app.log"
    },
    "osm_cache": {
        "enabled": true,
        "directory": "osm_cache",
        "debug": true
    },
    "files": [
        "data/fit/garmin-edge-1000-bigone.fit",
        "data/fit/garmin-edge-1000-bike.fit",
        "data/fit/garmin-fenix-3-run.fit",
        "data/gpx/navas-colmenar-fresnedillas-zarzalejo-robledo-navas.gpx",
        "data/gpx/picadas-pelayos-san-martin-y-vuelta.gpx"
    ]

}