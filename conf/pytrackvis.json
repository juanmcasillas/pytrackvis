{
    "verbose": true,
    "points": {
        "optimize": true,
        "filter": true
    },
    "database": {
        "file": "db/trackdb.db",
        "script": "db/trackdb.sql"
    },
    "proxy": {
        "enabled": false,
        "url": "http://proxy.senado.es:8080"
    },
    "tokens": {
        "MAPTILER_KEY": ""
    },
    "api_key_file": "conf/tokens.ini",
    "logs": {
        "level": "INFO",
        "format": "[%(asctime)s] %(levelname)s in %(module)s:%(funcName)s:%(lineno)d: %(message)s",
        "app": "logs/app.log"
    },
    "osm_cache": {
        "enabled": true,
        "directory": "osm_cache",
        "debug": true
    }
    
}